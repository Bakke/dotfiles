# Delete all metadata in a file
exiftool -all= <file_name>

# Fix datatime metadata from Google Takeout json
exiftool -d "%s" -tagsfromfile %d%f.%e.json "-DateTimeOriginal<PhotoTakenTimeTimestamp" "-FileCreateDate<PhotoTakenTimeTimestamp" "-FileModifyDate<PhotoTakenTimeTimestamp" -overwrite_original -ext AVIF -ext GIF -ext HEIC -ext html -ext jpeg -ext jpg -ext JPG -ext mov -ext MOV -ext mp4 -ext MP4 -ext png -ext PNG -ext WEBP -r .
